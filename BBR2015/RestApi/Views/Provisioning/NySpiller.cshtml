@model RestApi.Controllers.ProvisioningController.NySpillerModel

@{
    ViewBag.Title = "Bouvet Battle Royale - Ny spiller";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Ny deltaker</h2>
<form method="post">
 
    <div class="form-group">
        @Html.LabelFor(x => x.HemmeligKodeForLag) @Html.ValidationMessageFor(x => x.HemmeligKodeForLag)
        @Html.EditorFor(x => x.HemmeligKodeForLag, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(x => x.KodeForSpiller) @Html.ValidationMessageFor(x => x.KodeForSpiller)
        @Html.EditorFor(x => x.KodeForSpiller, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(x => x.Navn) @Html.ValidationMessageFor(x => x.Navn)
        @Html.EditorFor(x => x.Navn, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <input type="submit" value="Lagre" class="form-control btn btn-primary">
    </div>

</form>
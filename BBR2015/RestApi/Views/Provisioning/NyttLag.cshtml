@model RestApi.Controllers.ProvisioningController.NyttLagModel

@{
    ViewBag.Title = "Bouvet Battle Royale - Nytt lag";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Nytt lag</h2>
<form method="post">
    @Html.HiddenFor(x => x.MatchId)
    <div class="form-group">
        @Html.LabelFor(x => x.HemmeligKode) @Html.ValidationMessageFor(x => x.HemmeligKode)
        @Html.EditorFor(x => x.HemmeligKode, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(x => x.Navn) @Html.ValidationMessageFor(x => x.Navn)
        @Html.EditorFor(x => x.Navn, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <input type="submit" value="Lagre" class="form-control btn btn-primary">
    </div>

</form>